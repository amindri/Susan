@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Dashboard/_LayoutPageDash.cshtml";
}


<!--Custom css file combobox styles by Anu-->
@Styles.Render("~/Content/custom-styles.css")

<div class="container" id="dashboard"> 
    <h2>DASHBOARD - FIRST IN SPORTS FIRST AID</h2>
</div>
<hr />
